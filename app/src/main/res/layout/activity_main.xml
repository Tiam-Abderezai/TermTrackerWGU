<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/terms_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/button_terms"
        tools:layout_editor_absoluteX="57dp"
        tools:layout_editor_absoluteY="341dp" />

    <Button
        android:id="@+id/courses_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/button_courses"
        tools:layout_editor_absoluteX="243dp"
        tools:layout_editor_absoluteY="340dp" />

    <Button
        android:id="@+id/assessments_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/button_assessments"
        tools:layout_editor_absoluteX="57dp"
        tools:layout_editor_absoluteY="421dp" />

    <Button
        android:id="@+id/mentors_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/button_mentors"
        tools:layout_editor_absoluteX="243dp"
        tools:layout_editor_absoluteY="421dp" />
</LinearLayout>